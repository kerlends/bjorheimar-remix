{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "scripts": {
    "build": "run-s build:css build:server",
    "build:server": "remix build",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev": "run-p dev:*",
    "dev:server": "remix dev",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "generate": "prisma generate"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 80,
    "useTabs": true,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true
  },
  "dependencies": {
    "@prisma/client": "^3.5.0",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@remix-run/vercel": "^1.0.6",
    "@vercel/node": "^1.12.1",
    "cheerio": "^1.0.0-rc.10",
    "cloudinary": "^1.27.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.26.0",
    "dotenv": "^10.0.0",
    "fuzzyset": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/dotenv": "^8.2.0",
    "@types/fuzzyset": "^1.0.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.3",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.5.0",
    "prisma": "^3.5.0",
    "ramda": "^0.27.1",
    "tailwindcss": "^2.2.19",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}