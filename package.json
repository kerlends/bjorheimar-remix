{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "scripts": {
    "job:sync": "node ./jobs/sync.js",
    "build": "run-s build:css build:server",
    "build:server": "remix build",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "dev": "run-p dev:*",
    "dev:server": "node -r dotenv/config node_modules/.bin/remix dev",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "postinstall": "remix setup vercel",
    "start": "remix-serve api/build",
    "generate": "prisma generate",
    "format": "prettier \"./app/**/*.{ts,tsx,css}\" --write"
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 80,
    "useTabs": true,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@prisma/client": "^3.7.0",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/vercel": "^1.1.1",
    "@types/auth0": "^2.34.9",
    "@types/uuid": "^8.3.3",
    "@vercel/node": "^1.12.1",
    "auth0": "^2.37.0",
    "cheerio": "^1.0.0-rc.10",
    "cloudinary": "^1.27.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.26.0",
    "dotenv": "^10.0.0",
    "fuzzyset": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/fuzzyset": "^1.0.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.3",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.5.0",
    "prisma": "^3.7.0",
    "ramda": "^0.27.1",
    "tailwindcss": "^2.2.19",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
